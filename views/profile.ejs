    <%- include('partials/head',{title:'Profile'}) %>
  <div class="container">  <table class="profile-table">
      <tr>
        <th>Name</th>
        <td><%= user.name %></td>
      </tr>
      <tr>
        <th>Batch</th>
        <td><%= user.batch %></td>
      </tr>
    </table>
    <button class="logout-btn" onclick="logOut()">LOG OUT</button>
  </div>

  <script>
    function logOut() {
      location.replace('http://localhost:4000/logout');
    }
  </script>


<style>

body {
  font-family: sans-serif;
  margin: 50px;
}

.container {
  width: 300px; /* Adjust width as needed */
  margin: 0 auto;
  padding: 20px;
  border: 1px solid #ccc;
  border-radius: 5px;
}

.profile-table {
  width: 100%;
  border-collapse: collapse; /* Remove borders between cells */
}

.profile-table th,
.profile-table td {
  padding: 10px;
  border: 1px solid #ccc;
  text-align: left; /* Align content to the left */
}

.logout-btn {
  background-color: #4CAF50;
  color: white;
  padding: 10px 20px;
  border: none;
  border-radius: 3px;
  cursor: pointer;
  margin-top: 20px;
}

</style>
<%-include('partials/tail')%>
